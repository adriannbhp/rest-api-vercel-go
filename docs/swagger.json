    "definitions": {
        "controllers.CreateBookRequest": {
            "type": "object",
            "properties": {
                "description": {
                    "type": "string",
                    "example": "Deskripsi buku"
                },
                "image_url": {
                    "type": "string",
                    "example": "https://example.com/gambar.jpg"
                },
                "price": {
                    "type": "string",
                    "example": "29.99"
                },
                "release_year": {
                    "type": "integer",
